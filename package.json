{
  "name": "svelte-cli",
  "version": "2.0.0",
  "description": "Command line interface for Svelte",
  "main": "index.js",
  "bin": {
    "svelte": "bin.js"
  },
  "files": [
    "README.md",
    "bin.js"
  ],
  "scripts": {
    "test": "mocha test/test.js",
    "pretest": "npm run build",
    "build": "rollup -c",
